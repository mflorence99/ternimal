{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "e1797a63-90dc-4047-b3ed-706096956513"
  },
  "defaultProject": "ternimal",
  "newProjectRoot": "projects",
  "projects": {
    "ternimal": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "production": {
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10kb",
                  "maximumWarning": "6kb",
                  "type": "anyComponentStyle"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "vendorChunk": false
            }
          },
          "options": {
            "aot": true,
            "assets": [
              "renderer/assets"
            ],
            "index": "renderer/index.html",
            "main": "renderer/main.ts",
            "outputPath": "dist/renderer",
            "polyfills": "renderer/polyfills.ts",
            "styles": [
              "node_modules/typeface-b612-mono/index.css",
              "node_modules/xterm/css/xterm.css",
              "renderer/theme.scss"
            ],
            "tsConfig": "renderer/tsconfig.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ternimal:build"
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ternimal:build:production"
            }
          },
          "options": {
            "browserTarget": "ternimal:build"
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "renderer"
    }
  },
  "version": 1
}
